[
  {
    "path": "/chat/",
    "name": "PageChat",
    "template": "/chat/static/index.vue",
    "component": "/chat/static/index.js"
  },
  {
    "path": "/chat/:id",
    "name": "PageChatPublic",
    "template": "/chat/static/public_page.vue",
    "component": "/chat/static/public_page.js"
  },
  {
    "path": "/chat/:id/:chat",
    "name": "PageChatPublicChat",
    "template": "/chat/static/public_page.vue",
    "component": "/chat/static/public_page.js"
  },
  {
    "path": "/chat/embed/:id",
    "name": "PageChatEmbed",
    "template": "/chat/static/embed.vue",
    "component": "/chat/static/embed.js"
  },
  {
    "path": "/chat/embed/:id/:chat",
    "name": "PageChatEmbedChat",
    "template": "/chat/static/embed.vue",
    "component": "/chat/static/embed.js"
  }
]
